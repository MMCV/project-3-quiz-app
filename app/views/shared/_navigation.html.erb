<nav class="navbar navbar-default" role="navigation">
  <div class="container">
    <div class="navbar-header">
      <a class="navbar-brand" href="/">HOME</a>
    </div>
    <% if logged_in? %>
      <button type="button" onclick="location.href='/logout';" class="btn btn-default navbar-btn navbar-right">Log out</button>
      <p class="navbar-text">You are logged in as <%= current_user.first_name %> <%= current_user.last_name %></p>
    <% else %>
      <button type="button" onclick="location.href='/login';" class="btn btn-default navbar-btn navbar-right">Sign in</button>
    <% end %>
  </div>
</nav>
